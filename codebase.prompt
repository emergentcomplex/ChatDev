            self.x -= dx
            self.y -= dy
            self.rect.x = self.x
            self.rect.y = self.y
        elif level.is_door(self.x, self.y):
            level.next_level(self)
        elif level.is_treasure(self.x, self.y):
            self.hp += random.randint(20, 30)
    def draw(self, screen):
        pygame.draw.rect(screen, (255, 0, 0), self.rect)

