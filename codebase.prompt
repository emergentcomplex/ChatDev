'''
import sys
import random
class Player:
    def __init__(self, name, chips):
        self.name = name
        self.chips = chips
        self.hand = []
        self.community_cards = []
        self.active = True
        self.blind = 0
        self.previous_bet = 0
        self.is_in_showdown = True
    def receive_cards(self, cards):
        self.hand.extend(cards)
    def receive_community_cards(self, cards):
        self.community_cards = cards
    def is_active(self):
        return self.active
    def all_in(self):
        tmp = self.chips
        self.chips = 0
        self.active = False
        print(f"{self.name} is All In for {tmp}")
        return tmp
    def fold(self):
        print(f"{self.name} folded.")
        self.active = False
        self.is_in_showdown = False
    def check(self):
        prompt = f"{self.name} checked."
        print(prompt)
        return 0
    def call(self, amount):
        self.chips -= amount
        prompt = f"{self.name} called {amount}. Remaining chips: {self.chips}"
        print(prompt)
        return amount
    def raise_bet(self, amount):
        self.chips -= amount
        prompt = f"{self.name} raised an additonal {amount}. Remaining chips: {self.chips}"
        print(prompt)
        return amount
